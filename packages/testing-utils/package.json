{
  "name": "@autoqa/testing-utils",
  "version": "1.0.0",
  "description": "Testing utilities and helpers for AutoQA Pilot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:property": "jest --selectProjects property",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "fast-check": "^3.15.0",
    "jest-extended": "^4.0.2",
    "@testcontainers/postgresql": "^10.4.0",
    "@testcontainers/redis": "^10.4.0",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "minio": "^7.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/pg": "^8.10.7",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "jest": "^29.0.0"
  }
}